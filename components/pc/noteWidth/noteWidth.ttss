.noteWidthBox{
    width: 100%;
    height: 120px;
    background: rgba(82,115,210);
    position: fixed;
    bottom: 0px;
    border-radius: 0 0 5px 5px;
    z-index: 400;
}
.noteWidthBox_con{
    width: 100%;
    height: 100%;
    color: #fff;
    font-size: 15px;
    text-align: center;
    line-height: 120px;
    position: relative;
}
.noteWidthBox_con_text{
    color: gray;
}
.noteWidthBox_con_img{
    position: absolute;
    right: 20px;
    top: 52px;
    width: 20px;
    height: 20px;
}