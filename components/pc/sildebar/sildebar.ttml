 <view class="sildeBox">
                <block tt:for="{{list}}" tt:key="index">
                    <view class="silde_i {{activeType==item.type?'sidebarActive':''}}" data-type="{{item.type}}" catchtap="selectSildbar">
                        <image class="silde_i_img" src="{{item.imgSrc}}"></image>
