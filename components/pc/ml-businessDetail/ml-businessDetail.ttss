  margin-left: 33px;
}

.textarea-class{
  padding:0 !important;
  height: 54px !important;
}

.select-address{
  width: 100%;
  height: 36px;
  display: flex;
  align-items: center;
  margin-bottom: 21px;

}

.select-class{
  width: 88px !important;
  height: 36px !important;
  margin-right: 7px;

}

